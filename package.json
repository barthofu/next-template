  },
  "homepage": "https://github.com/barthofu/starters#readme",
  "dependencies": {
    "@octokit/request": "^6.2.2",
    "axios": "^1.2.0",
    "boxen": "^7.0.0",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "inquirer": "^8.0.1",
    "log-symbols": "^3.0.0",
    "ora": "^5.1.2",
